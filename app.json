{
  "expo": {
    "name": "mercadomy",
    "slug": "mercadomy",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "src/assets/images/icon.png",
    "scheme": "mercadomy",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.mercadomy"
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#E6F4FE",
        "foregroundImage": "src/assets/images/android-icon-foreground.png",
        "backgroundImage": "src/assets/images/android-icon-background.png",
        "monochromeImage": "src/assets/images/android-icon-monochrome.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.mercadomy"
    },
    "web": {
      "output": "static",
      "favicon": "src/assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "src/assets/images/logo1.jpg",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#01AC66",
          "dark": {
            "backgroundColor": "#01AC66"
          }
        }
      ],
      [
        "expo-font",
        {
          "fonts": [
            "src/assets/fonts/inter/Inter-ExtraLight.otf",
            "src/assets/fonts/inter/Inter-Light.otf",
            "src/assets/fonts/inter/Inter-Regular.otf",
            "src/assets/fonts/inter/Inter-Medium.otf",
            "src/assets/fonts/inter/Inter-SemiBold.otf",
            "src/assets/fonts/inter/Inter-Bold.otf",
            "src/assets/fonts/inter/Inter-ExtraBold.otf",
            "src/assets/fonts/inter/Inter-Black.otf"
          ]
        }
      ],
      "expo-localization"
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    }
  }
}
